# Frontend Environment Variables
# Copy this file to .env.frontend and update with your deployment values

# Backend API URL - Change this based on where your backend is deployed
# For local development:
BACKEND_URL=http://localhost:8000/api

# For server deployment, use your server's address:
# BACKEND_URL=http://your-server-domain.com:8000/api
# OR
# BACKEND_URL=https://your-server-domain.com/api

# API Request Timeout (in milliseconds)
API_TIMEOUT=30000
